div
  &.tagsinput
    padding: 5px 10px
    padding-bottom: 0
    overflow-y auto
    span
      &.tag
        border 1px solid #9DACA9
        border-radius 3px
        display block
        float left
        padding 5px
        text-decoration none
        background #A8BAC5
        color white
        margin-right 5px
        margin-bottom 5px
        font-size 0.8em
        a
          font-size: 0.6em
    input
      width 80px
      margin 0px
      font-family helvetica
      font-size 13px!important
      border 1px solid transparent
      padding 5px
      background transparent
      color #000
      outline 0px
      margin-right 5px
      margin-bottom 5px
      position: relative
      top: auto
      left: auto
      display: block
    div
      display block
      float left

.tags_clear
  clear both
  width 100%
  height 0px

.not_valid
  background #FBD8DB !important
  color #90111A !important
  

.ac_results
  padding: 0px
  border: 1px solid GRAY_1
  background-color: white
  overflow: hidden
  z-index: 99999


  ul
    width: 100%
    list-style-position: outside
    list-style: none
    padding: 0
    margin: 0


  li
    margin: 0px
    padding: 7px 5px
    cursor: default
    display: block
    font-size: .8em
    line-height: 1em
    overflow: hidden

.ac_odd
  background-color: #eee


.ac_over
  background-color: #A8BAC5
  color: white



